<p><%= current_user.username %></p>
<p><%= link_to "New", new_post_path %></p>

<% @posts.each do |post| %>
	<div class="posts">
		<h4><%= link_to post.title, post_path(post) %></h4>
		<p><%= post.body %></p>
		<p><%= link_to "Edit", edit_post_path(post) %> / <%= link_to "Delete", post_path(post), method: "delete" %></p>
	</div>
<% end %>

<%= render 'form' %>
